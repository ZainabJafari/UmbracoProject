{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "6a320c85-6779-4f46-9aa6-dda2b86d5358",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Runtime": {
        "Mode": "Production"
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Unattended": {
        "UpgradeUnattended": true
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=tcp:myumbracoprojectserver.database.windows.net,1433;Database=Umbraco_database;User ID=hans@onatrix.com;Password=BytMig123!",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}
